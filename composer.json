{
    "name": "joomla/api-documentation",
    "description": "Application code for api.joomla.org",
    "homepage": "http://github.com/joomla/api.joomla.org",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "illuminate/console": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/filesystem": "5.7.*",
        "joomla/application": "~2.0@dev",
        "joomla/console": "~2.0@dev",
        "joomla/database": "~2.0@dev",
        "joomla/di": "~2.0@dev",
        "joomla/event": "~2.0@dev",
        "joomla/filter": "~2.0@dev",
        "joomla/input": "~2.0@dev",
        "joomla/registry": "~2.0@dev",
        "monolog/monolog": "^1.23",
        "phpdocumentor/reflection": "~3.0",
        "symfony/console": "4.1.*",
        "symfony/process": "4.1.*",
        "symfony/yaml": "4.1.*"
    },
    "require-dev": {
        "joomla/coding-standards": "dev-master",
        "squizlabs/php_codesniffer": "^2.9"
    },
    "replace": {
        "paragonie/random_compat": "*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\ApiDocumentation\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.21"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
